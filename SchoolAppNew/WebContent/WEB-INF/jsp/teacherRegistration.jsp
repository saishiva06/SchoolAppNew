	<fieldset>
		<h2 align="center">Teacher Registration:</h2>
		<form name="SchoolInfo" action="#" method="post">
			<p align="center">Please Fill the following details to submit Teacher
				details.</p>
				<p align="left"><a href="teacher.do"><input type="button" name="add" value="BACK" ></input></a></p>
			<table>
				
				<tr>
					<td align="right"><label>First Name*: </label></td>
					<td><input id="name" name="studentFirstName" type="text" /></td>
				</tr>
				<tr>
					<td align="right"><label>Last Name*: </label></td>
					<td><input id="name" name="studentLastName" type="text" /></td>
				</tr>
